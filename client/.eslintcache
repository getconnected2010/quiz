</div>\r\n\r\n                            <div className='a'>\r\n                                <label>{qa.answer4}</label>\r\n                                <input type=\"radio\" name={qa.question+qa.id} value={qa.answer4===qa.correct} />\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n                ))\r\n            }\r\n            {quiz.length>0 && <button type='submit' onClick={handleSubmit}>Submit Answers</button>}\r\n        \r\n        </form>\r\n    </div>\r\n    )\r\n}\r\nexport default QaList;","C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\components\\Home.js",[],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\components\\Nav.js",[],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\components\\add.js",[],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\reducers\\quizReducer.js",[],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\components\\Signup.js",["61"],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\reducers\\userReducer.js",[],"C:\\Users\\haile\\OneDrive\\Documents\\projects\\quiz\\client\\src\\actions\\userActions.js",[],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":24,"column":15,"nodeType":"68","messageId":"69","endLine":24,"endColumn":21},{"ruleId":"66","severity":1,"message":"70","line":9,"column":11,"nodeType":"68","messageId":"69","endLine":9,"endColumn":16},{"ruleId":"66","severity":1,"message":"71","line":2,"column":44,"nodeType":"68","messageId":"69","endLine":2,"endColumn":58},"no-native-reassign",["72"],"no-negated-in-lhs",["73"],"no-unused-vars","'result' is assigned a value but never used.","Identifier","unusedVar","'admin' is assigned a value but never used.","'FormikProvider' is defined but never used.","no-global-assign","no-unsafe-negation"]